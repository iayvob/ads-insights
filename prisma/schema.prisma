generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id            String    @id @default(auto()) @map("_id") @db.ObjectId
  adAccountId   String?
  name          String?
  email         String?   
  image         String?
  lastLoginAt   DateTime?
  createdAt     DateTime  @default(now())
}